<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/caldroid_white" />
        </shape>
    </item>
    <item
        android:width="12dp"
        android:height="12dp"
        android:gravity="right"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <solid android:color="@color/sankalp_tyag" />
        </shape>
    </item>
    <item
        android:width="12dp"
        android:height="12dp"
        android:bottom="5dp"
        android:gravity="bottom|right"
        android:right="5dp">>
        <shape android:shape="oval">
            <solid android:color="@color/sankalp_niyam" />
        </shape>
    </item>

</layer-list>