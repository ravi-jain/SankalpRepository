    <style name="SpHorizontalDivider">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">#888888</item>
        <item name="android:layout_height">1dip</item>
    </style>

    <style name="SpLabel2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:color">#737270</item>
        <item name="android:fontFamily" tools:targetApi="jelly_bean">sans-serif-condensed</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="SpText2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily" tools:targetApi="jelly_bean">sans-serif-condensed</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="SpText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

    <style name="SpTextCardLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
    </style>

    <style name="SpLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:color">@color/sankalp_grey</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>

